//Java Solution:-

class Solution {
    public int search(int[] nums, int target) {
        int len=nums.length;
        int a=0,flag=0;
        for(int i=0;i<len;i++)
        {
            if(nums[i]==target)
            {
                a=i;
                flag=1;
                break;
            }            
        }
        if(flag==1)
        return a;
        else
        return -1;
        
    }
}
